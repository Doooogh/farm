# JWT
JWT_SECRET=your-secret-key-your-secret-key-your-secret-key
JWT_EXPIRATION=3600
JWT_TOKEN_EXPIRATION=24
JWT_REFRESH_TOKEN_EXPIRATION=7
JWT_ISSUER=com.doooogh.farm

# Spring Profile
SPRING_PROFILES_ACTIVE=dev

# OAuth2 GitHub
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# OAuth2 Custom
CUSTOM_CLIENT_ID=farm-client
CUSTOM_CLIENT_SECRET=farm-secret
CUSTOM_AUTH_URI=http://localhost:8081/oauth2/authorize
CUSTOM_TOKEN_URI=http://localhost:8081/oauth2/token
CUSTOM_USER_INFO_URI=http://localhost:8081/userinfo
CUSTOM_JWK_SET_URI=http://localhost:8081/.well-known/jwks.json

NACOS_GRPC_NETTY_TRANSPORT=nio