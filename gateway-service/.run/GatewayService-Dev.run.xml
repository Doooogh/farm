<component name="ProjectRunConfigurationManager">
  <configuration default="false" name="GatewayService-Dev" type="SpringBootApplication">
    <option name="MAIN_CLASS_NAME" value="com.doooogh.farm.gateway.GatewayServiceApplication" />
    <module name="gateway-service" />
    <option name="WORKING_DIRECTORY" value="$PROJECT_DIR$" />
    <option name="VM_PARAMETERS" value="-Dspring.profiles.active=dev" />
    <envs>
      <!-- 环境变量配置 -->
      <env name="REDIS_HOST_DEV" value="localhost" />
      <env name="REDIS_PORT_DEV" value="6379" />
      <!-- ... 其他环境变量 ... -->
    </envs>
    <extension name="net.ashald.envfile">
      <option name="IS_ENABLED" value="true" />
      <option name="IS_SUBST" value="false" />
      <option name="IS_PATH_MACRO_SUPPORTED" value="false" />
      <option name="IS_IGNORE_MISSING_FILES" value="false" />
      <option name="IS_ENABLE_EXPERIMENTAL_INTEGRATIONS" value="false" />
      <ENTRIES>
        <ENTRY IS_ENABLED="true" PARSER="runconfig" />
        <ENTRY IS_ENABLED="true" PARSER="env" PATH="$MODULE_DIR$/../.env" />
      </ENTRIES>
    </extension>
    <method v="2">
      <option name="Make" enabled="true" />
    </method>
  </configuration>
</component> 